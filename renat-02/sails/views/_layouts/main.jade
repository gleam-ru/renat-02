include ../_mixins/global

doctype
html.wide.wow-animation.smoothscroll
  head
    block head
      include ./parts/_head
    title
      block pageTitle
        | #{pageTitle ? pageTitle+' - ' : title ? title+' - ' : ''}
        | ReliantMC
    body#body
      +globalVar('defaultDateFormat', ddf)
      block body
        .page.text-center
          header.page-head
            //- navbar
            .rd-navbar-wrap
              nav.rd-navbar#menu(
                data-layout='rd-navbar-fixed',
                data-sm-layout='rd-navbar-fixed',
                data-lg-layout='rd-navbar-static',
                data-sm-stick-up-offset='31px',
                data-lg-stick-up-offset='41px',
                data-xl-stick-up-offset='41px',
                data-md-layout='rd-navbar-fullwidth',
                data-md-device-layout='rd-navbar-fixed', 
                data-lg-device-layout='rd-navbar-fixed'
                )
                .rd-navbar-inner
                  .rd-navbar-outher
                    // RD Navbar Panel
                    .rd-navbar-panel
                      // RD Navbar Toggle
                      button.rd-navbar-toggle(
                        data-rd-navbar-toggle='.rd-navbar-nav-wrap'
                        )
                        span
                      button.rd-navbar-collapse-toggle(
                        data-rd-navbar-toggle='.rd-navbar-collapse'
                        )
                        span
                      // RD Navbar Brand
                      .rd-navbar-brand
                        a.brand-name(href='#')
                          img(src='images/logo.png', width='268', height='57')
                      .rd-navbar-brand-mobile
                        a.brand-name(href='#')
                          img(src='images/logo.png', width='268', height='57')
                    .rd-navbar-nav-wrap
                      // RD Navbar Nav
                      ul.rd-navbar-nav
                        li.active(data-menuanchor="home")
                          a(href='#home') Home
                        li.rd-navbar--has-dropdown.rd-navbar-submenu(data-menuanchor="business-in-uae")
                          //- @fixme - пока нет контента скрываю костылями.
                          //- a(href='static/business-in-uae/') Business In UAE
                          a(href='#business-in-uae') Business In UAE
                          ul.rd-navbar-dropdown
                            li
                              a(href='static/business-in-uae/') Company setup
                            li
                              a(href='static/business-in-uae/business-without-taxes/') Organization of business
                            li
                              a(href='static/business-in-uae/access-to-international-market/') International market
                            li
                              a(href='static/business-in-uae/manufacturing/') Manufacturing
                            li
                              a(href='static/business-in-uae/investment-in-uae/') Investment In UAE
                          span.rd-navbar-submenu-toggle
                        li.rd-navbar--has-dropdown.rd-navbar-submenu(data-menuanchor="company-registration")
                          //- @fixme - пока нет контента скрываю костылями.
                          //- a(href='static/company-registration/') Company registration
                          a(href='#company-registration') Company registration
                          ul.rd-navbar-dropdown
                            li
                              a(href='static/company-registration/local-company/') Local company
                            li
                              a(href='static/company-registration/free-zone-company/') Free zone company
                            li
                              a(href='static/company-registration/offshore-ibc/') Offshore-ibc
                            li
                              a(href='static/company-registration/holding-structures/') Holding structures
                            li
                              a(href='static/company-registration/complicated-license/') Complicated licenses
                            li
                              a(href='static/company-registration/joint-venture/') Joint venture
                          span.rd-navbar-submenu-toggle
                        li.rd-navbar--has-dropdown.rd-navbar-submenu(data-menuanchor="business-management")
                          a(href='#business-management') Business management
                          ul.rd-navbar-dropdown
                            li
                              a(href='static/business-management/management-consulting/') Management consulting
                            li
                              a(href='static/business-management/legal-assistance/') Legal assistance
                            li
                              a(href='static/business-management/open-bank-accounts/') Open bank accounts
                          span.rd-navbar-submenu-toggle
                        li.rd-navbar--has-dropdown.rd-navbar-submenu(data-menuanchor="capabilities")
                          a(href='#capabilities') Capabilities
                          ul.rd-navbar-dropdown
                            li
                              a(href='static/capabilities/sales-and-marketing/') Sales and marketing
                            li
                              a(href='static/capabilities/finance-and-deveopment/') Finance & development
                            li
                              a(href='static/capabilities/diversification/') Diversification
                          span.rd-navbar-submenu-toggle
                        li.mr-10(data-menuanchor="contacts")
                          a(href='#contacts') Contact us
                        li.extra-link
                          a(href='#')
                            i.fa.fa-facebook-square
                          a(href='#')
                            i.fa.fa-twitter-square
                          a(href='#')
                            i.fa.fa-google-plus-square
                  //- @todo
                  //- include ./parts/navigation

          main.page-content
            //- include ./parts/topnav
            //- include ./parts/breadcrumbs
            block content
          block footer
            footer.page-foot.text-sm-left.section-50.section-lg-90.offset-md-top--40
              .shell
                .range
                  .cell-md-5.offset-top-10
                    a.brand(href='')
                      img(src='images/logo.png', alt='', width='266', height='55')
                    p
                      | Investment products and services available only to residents of : Arizona - AZ, California - CA, Hawaii - HI, Idaho - ID, New Jersey - NJ, New Mexico - NM, Oregon - OR, Washington - WA Securities and Investment Advice through John Doe & Company, A registered broker/dealer and Investment Advisor, Member FINRA and SIPC. 101 Serious Road, Fincity, FL 32707 Tel: (959) 603- 6035.| John Doe Financial Advisors is not an affiliate of Wall Street Company.
                    p
                      | All other trademarks and copyrights are the properties of their respective holders. This website provides information related to the subjects covered. Before making any financial or legal decisions, a professional should be consulted.
                  .cell-xs-8.cell-xs-preffix-3.cell-sm-6.cell-sm-preffix-0.cell-md-3.cell-md-preffix-1.text-left.offset-top-50.offset-md-top-0
                    h3.text-medium Business in UAE
                    ul.marked-list
                      li
                        a(href='static/business-in-uae#tabs1') Business in UAE
                      li
                        a(href='static/business-in-uae#tabs2') Company Start Up
                      li
                        a(href='static/business-in-uae#tabs3') Register a company in UAE
                      li
                        a(href='static/business-in-uae#tabs4') Choice of activity
                      li
                        a(href='static/business-in-uae#tabs5') Choice of organization
                      li
                        a(href='static/business-in-uae#tabs6') Choice of location
                      li
                        a(href='static/business-in-uae#tabs7') Services for opening
                  .cell-xs-6.cell-xs-preffix-3.cell-sm-6.cell-sm-preffix-0.cell-md-3.offset-top-50.offset-md-top-0.text-left
                    h3.text-medium Industries
                    ul.marked-list
                      li
                        a(href='static/industries#tabs1') Production
                      li
                        a(href='static/industries#tabs2') Benefits of opening UAE
                      li
                        a(href='static/industries#tabs3') Unique persectives
                      li
                        a(href='static/industries#tabs4') Open proceedings
                      li
                        a(href='static/industries#tabs5') Benefits of manufacturing
                      li
                        a(href='static/industries#tabs6') Choise of FEZ
                      li
                        a(href='static/industries#tabs7') Leasing services
                      li
                        a(href='static/industries#tabs8') Benefits of leasing
                p.offset-top-50
                  | Copyright ©  
                  span#copyright-year
                  |   
                  a(href='')  Privacy Policy
                  // {%FOOTER_LINK}
    


    //- исходники шаблона
    script(src='js/tpl/core.min.js')
    script(src='js/tpl/script.js')
